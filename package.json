{
  "name": "learning",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:client": "webpack --config ./client/webpack/prod.config.js",
    "start:server:client": "webpack serve --config ./client/webpack/webpack.config.js --mode=development",
    "build:todoapp": "cd todoapp && tsc",
    "start:todoapp:dev": "cross-env NODE_ENV=development PORT=3001 nodemon --config ./todoapp/nodemon.dev.json",
    "start:todoapp:prod": "cross-env NODE_ENV=production PROXY_REVERSE_PASS=/dev nodemon --config ./todoapp/nodemon.prod.json",
    "start:todoapp:prod:js": "npm run build:todoapp && node ./dist/bin/www.js",
    "start:todoapp:independence": "ts-node --files ./todoapp/src/bin/www.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "connect-flash": "^0.1.1",
